  "ExpressJS Web Server Template": {
    "prefix": "tmp-express-web-server",
    "description": "Custom template for web server written in ExpressJS",
    "body": [
      "import { Command } from 'commander';",
      "import inquirer from 'inquirer';",
      "import express from 'express';",
      "",
      "import { dirname } from 'path';",
      "import { fileURLToPath } from 'url';",
      "const __dirname = dirname(fileURLToPath(import.meta.url));",
      "",
      "const app = express();",
      "",
      "const program = new Command();",
      "program",
      "  .name('$1')",
      "  .description('$2')",
      "  .version('1.0.0', '-v, --version', 'display version')",
      "  .option('-p, --port <port>', 'provide port to listen on')",
      "  .parse();",
      "",
      "inquirer",
      "  .prompt(",
      "    [{",
      "      type: 'number',",
      "      name: 'port',",
      "      message: 'provide port to listen on',",
      "      default: 3000",
      "    }],",
      "    program.opts()",
      "  ).then((options) => {",
      "    app.listen(options.port, () => {",
      "      console.log(`Express server running on port ${options.port}.`);",
      "    });",
      "  });",
      "",
      "app.get('/', (req, res) => {",
      "  console.log(req);",
      "  res.sendFile(__dirname + '/$3');",
      "});"
    ]
  }
}
